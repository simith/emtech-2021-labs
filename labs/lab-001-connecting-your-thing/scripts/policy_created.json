    "policyDocument": "{\n  \"Version\": \"2012-10-17\",\n  \"Statement\": [{\n      \"Effect\": \"Allow\",\n      \"Action\": \"iot:Connect\",\n      \"Resource\": \"arn:aws:iot:*:657566545454:client/${iot:Connection.Thing.ThingName}\",\n      \"Condition\": {\n        \"Bool\": {\n          \"iot:Connection.Thing.IsAttached\": [\n            \"true\"\n          ]\n        }\n      }\n    },\n    {\n      \"Effect\": \"Allow\",\n      \"Action\": \"iot:Publish\",\n      \"Resource\": [\n        \"arn:aws:iot:*:657566545454:topic/$aws/things/${iot:Connection.Thing.ThingName}/jobs/*\",\n        \"arn:aws:iot:*:657566545454:topic/$aws/things/${iot:Connection.Thing.ThingName}/shadow/*\",\n         \"arn:aws:iot:*:657566545454:topic/$aws/things/${iot:Connection.Thing.ThingName}/streams/*\"\n      ]\n    },\n    {\n      \"Effect\": \"Allow\",\n      \"Action\": \"iot:Subscribe\",\n      \"Resource\": [\n        \"arn:aws:iot:*:657566545454:topicfilter/$aws/things/${iot:Connection.Thing.ThingName}/jobs/*\",\n        \"arn:aws:iot:*:657566545454:topicfilter/$aws/things/${iot:Connection.Thing.ThingName}/shadow/*\",\n        \"arn:aws:iot:*:657566545454:topicfilter/$aws/things/${iot:Connection.Thing.ThingName}/streams/*\"\n\n        \n      ]\n    },\n    {\n      \"Effect\": \"Allow\",\n      \"Action\": \"iot:Receive\",\n      \"Resource\": [\n        \"arn:aws:iot:*:657566545454:topic/$aws/things/${iot:Connection.Thing.ThingName}/jobs/*\",\n        \"arn:aws:iot:*:657566545454:topic/$aws/things/${iot:Connection.Thing.ThingName}/shadow/*\",\n        \"arn:aws:iot:*:657566545454:topic/$aws/things/${iot:Connection.Thing.ThingName}/streams/*\"  \n      ]\n    }\n  ]\n}\n",
    "policyVersionId": "1"
}
